{
  "name": "mobile-template",
  "version": "0.0.0",
  "description": "template for mobile website",
  "scripts": {
    "start": "cross-env NODE_ENV=development npx mlz-pack serve",
    "build": "cross-env NODE_ENV=production npx mlz-pack build",
    "production": "cross-env NODE_ENV=production node bin/server.js",
    "example": "cross-env NODE_ENV=development npx mlz-pack serve example/index.tsx"
  },
  "author": "juice cube",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && env-status-version-validate",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,ts,tsx}": [
        "eslint",
        "git add"
      ],
      "*.{css,scss}": [
        "stylelint"
      ]
    },
    "ignore": [
      "bin/server.js",
      "src/lib/**/*",
      "mlz-pack.js",
      "*.test.(j|t)s"
    ]
  },
  "dependencies": {
    "@mlz/axios": "^0.2.9",
    "@mlz/super-utils": "^1.0.5",
    "classnames": "^2.2.6",
    "rdx-model": "0.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@mlz/lint": "^1.1.10",
    "@mlz/pack": "^0.1.14",
    "@types/classnames": "^2.2.9",
    "@types/node": "^12.6.2",
    "@types/react-css-modules": "^4.6.2",
    "@types/react-dom": "^16.9.2",
    "@types/react-loadable": "^5.5.2",
    "@types/react-redux": "^7.1.4",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^5.1.0",
    "compression": "^1.7.4",
    "cross-env": "^5.2.1",
    "env-status": "^0.30.0",
    "express": "^4.17.3",
    "husky": "^3.0.9",
    "lint-staged": "^8.0.4",
    "mlz-lint": "^1.0.2",
    "typescript": "^3.5.3"
  }
}
