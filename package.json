{
  "name": "mobile-template",
  "version": "0.0.0",
  "description": "template for mobile website",
  "scripts": {
    "start": "cross-env NODE_ENV=development mlz-pack serve",
    "build": "cross-env NODE_ENV=production mlz-pack build",
    "production": "cross-env NODE_ENV=production node bin/server.js",
    "lint": "tslint -p "
  },
  "author": "juice cube",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && env-status-version-validate",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,ts,tsx}": [
        "tslint",
        "git add"
      ]
    },
    "ignore": [
      "bin/server.js",
      "src/lib/**/*",
      "*.test.(j|t)s"
    ]
  },
  "dependencies": {
    "@mlz/axios": "^0.2.9",
    "classnames": "^2.2.6",
    "rdx-model": "0.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@mlz/lint": "^1.0.9",
    "@mlz/pack": "0.0.15",
    "@types/classnames": "^2.2.9",
    "@types/node": "^12.6.2",
    "@types/react-css-modules": "^4.6.2",
    "@types/react-dom": "^16.9.2",
    "@types/react-loadable": "^5.5.2",
    "@types/react-redux": "^7.1.4",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^5.1.0",
    "cross-env": "^5.2.1",
    "env-status": "^0.30.0",
    "express": "^4.17.1",
    "husky": "^3.0.9",
    "lint-staged": "^8.0.4",
    "mlz-lint": "^1.0.2",
    "tslint": "^6.0.0-beta0",
    "typescript": "^3.5.3"
  }
}
