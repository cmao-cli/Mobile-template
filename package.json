{
  "name": "mobile-template",
  "version": "0.0.0",
  "description": "template for mobile website",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --config webpack.dev.config.js",
    "start": "./script/dev_server.sh",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "start:prod": "node ./bin/product_server.js",
    "dll": "cross-env NODE_ENV=development webpack --progress --colors --config webpack/webpack.dll.config.js",
    "stats": "webpack --config webpack.config.js -p --progress --json > stats.json",
    "lint": "./node_modules/tslint/bin/tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'",
    "unit_test": "./node_modules/jest/bin/jest.js --color",
    "test": "tslint -c tslint.json -p tsconfig.json"
  },
  "author": "juice cube",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.2.6",
    "rdx-model": "0.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/node": "^12.6.2",
    "@types/react-dom": "^16.9.2",
    "@types/react-loadable": "^5.5.2",
    "@types/react-redux": "^7.1.4",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^5.1.0",
    "cross-env": "^5.2.1",
    "env-status": "^0.30.0",
    "express": "^4.17.1",
    "mlz-lint": "^1.0.2",
    "typescript": "^3.5.3"
  }
}
